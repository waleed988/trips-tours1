<%= stylesheet_link_tag "users" %>
<%= render partial: 'home/navbar'%>
<div class='container text-center effect devise'>
<h2 class = 'mb-5' style='color:#00095e;'><b>Log in</b></h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :email ,style:'color:#00095e;' %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :password,style:'color:#00095e;' %><br />
    <%= f.password_field :password, autocomplete: "current-password" %>
  </div>

  <div class="actions mt-5 ">
    <%= f.submit "Log in",class:'login100-form-btn' %>
  </div>
<% end %>
<%= render "devise/shared/links" %>

</div>

<%= render partial: 'home/footer'%>

