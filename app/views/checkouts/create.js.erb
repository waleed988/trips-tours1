
var stripe = Stripe("<%= Rails.application.credentials[:stripe][:public] %>");

stripe.redirectToCheckout({
  sessionId: '<%= @session.id %>'
}).then(function (result) {
  <% flash[:notice] = "You have Booked successfully." %>
}).catch(() => {
  <% flash[:notice] = nil %>
  <% flash[:alert] = "Booking fail." %>
});

